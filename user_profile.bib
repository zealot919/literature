@Article{2016-springer-Efficient,
  author   = {S. Berretti},
  title    = {Efficient User Profiling in Twitter Social Network Using Traditional Classifiers},
  journal  = {Intelligent Systems Technologies and Applications},
  year     = {2016},
  abstract = {如果参与讨论的人与一个领域有关，社交媒体的任何讨论都会有成果。以类似的方式宣传活动，查找对活动内容感兴趣的用户很有用。在包含大量用户的Twitter等社交网络中，根据用户的兴趣对用户进行分类将有助于此原因。本文提出了一种有效的监督机器学习方法，它将Twitter用户基于三个重要特征（基于Twitter的，基于用户的和基于时间的系列）分为六个兴趣类别 - 政治，娱乐，创业，新闻，科学与医疗 。我们将提出的特征集与支持向量机，朴素贝叶斯，k最近邻，决策树和逻辑回归等不同的传统分类器进行比较，并获得高达89.82％的分类精度。我们还提出了一个用于Twitter用户分析的实时系统的设计以及原型实现},
  file     = {2016-springer-Efficient User Profiling in Twitter Social Network Using Traditional Classifiers.pdf:2016-springer-Efficient User Profiling in Twitter Social Network Using Traditional Classifiers.pdf:PDF},
}

@Article{14354237257139HI2QBQJLI,
  author   = {Yutao_Zhang},
  title    = {COSNET: Connecting Heterogeneous Social Networks with Local and Global Consistency COSNET：连接异构社会网络与本地和全球一致性},
  journal  = {KDD},
  year     = {2015},
  abstract = {人们往往活跃在不止一个社交网络中。 从多个异构社交网络中识别用户并集成不同的网络是许多应用程序的根本问题。 现有方法通过估计两个网络中的用户之间的成对相似性来解决这个问题。 然而，这些方法存在多个网络之间的匹配潜在的不一致。在本文中，我们提出了COSNET（通过本地和全球一致性连接异构社交NETworks），一种新型的基于能量的模型，通过考虑多个网络之间的本地和全局一致性来解决这个问题。开发了一种有效的子梯度算法，通过将原有的基于能量的目标函数转化为双重形式来训练模型。我们对两种不同类型的数据收集模型进行评估：SNS和学术界，每个都由多个异构社会网络组成。我们的实验结果验证了提出的模型的有效性和效率。在两个数据收集中，拟议的COSNET方法在F1分数方面显着优于多种替代方法，高达10-30％（p <0：001，t检验）。我们还表明，应用我们的方法产生的整合结果可以提高专家发现的准确性，这是社交网络中的一项重要任务。},
  file     = {14354237257139HI2QBQJLI.pdf:14354237257139HI2QBQJLI.pdf:PDF},
  keywords = {rank1},
  review   = {链接特定用户不同社交网络账户
美国的一项调查显示，三分之二的在线成人（66％）使用社交媒体平台，如Facebook，Twitter，MySpace或LinkedIn，以保持联系目前的朋友，家人和商业伙伴。用户生成异构内容，并在不同的社交网络中建立不同的自我网络。一个有趣和重要的问题是：我们可以自动将不同的异构社交网络整合在一起吗？
用户的行为是一个重要的线索（也称为社交指纹[40]  ###p41### ），以帮助识别不同网络中的用户。
如果我们使用相似度s（u; v）; u 2 G1; v 2 G2在用户之间链接来自两个不同网络（G1和G2）的用户，该问题可以被形式化为一个优化问题：maxΣu; v s（u; v）。通过使用最小成本流算法可以有效解决问题[1,34]。


现在我们解释为什么定义的能量函数在方程式 1可以利用网络匹配的想法。假设“邻域保留匹配”的原则在现实世界的数据集中是有效的。在我们从训练数据中学习模型参数后，yi和yj的权重相同，将很低。对于测试集，如果在匹配图中连接了两个用户对，如果分配相同的标签，模型能量将会很低。因此，通过网络匹配能量模型的定义在等式 1，我们倾向于为匹配图中的邻居分配相同的标签，以最小化模型能量。},
}

@Article{p41,
  author   = {Reza_Zafarani and Huan Liu},
  title    = {Connecting Users across Social Media Sites: A Behavioral-Modeling Approach},
  journal  = {KDD},
  year     = {2013},
  abstract = {人们为了不同的目的使用各种社交媒体。个别网站上的信息通常不完整。当补充信息的来源被整合时，可以建立更好的用户简档来改善在线服务，例如验证在线信息。要整合这些信息来源，有必要在社交媒体网站上识别个人。本文旨在解决跨媒体用户识别问题。我们介绍一种方法（MOBIUS），用于在社交媒体网站上查找个人身份的映射。它由三个关键组件组成：第一个组件识别用户的独特行为模式，导致跨站点的信息冗余;第二个组件构造了由于这些行为模式而利用信息冗余的特征;第三个组件采用机器学习进行有效的用户识别。我们正式定义跨媒体用户识别问题，并显示MOBIUS有效地识别跨社交媒体网站的用户。这项研究为跨社交媒体网站的分析和挖掘铺平了道路，并有助于跨网站创建新颖的在线服务。},
  file     = {p41.pdf:p41.pdf:PDF},
  review   = {用户跨媒体多账户识别  虚假信息识别 用户的行为是一个重要的线索（也称为社交指纹[本文]），以帮助识别不同网络中的用户。
当提供虚假信息（例如，不正确的年龄）时，可能会在各个站点之间产生信息不一致。检测这些不一致可以帮助提供解决年龄验证问题的第一线安全性。检测这些不一致之处的一种方法是开始将用户的不同身份连接到社交媒体网站。

在社交媒体网站上连接用户身份并不是一件简单的工作。主要障碍是跨不同站点的用户身份之间的连接通常不可用。这种断开发生，因为大多数网站通过允许用户自由选择用户名而不是其真实身份，并且还因为不同的网站使用不同的用户命名和身份验证系统来保持用户的匿名性。此外，网站很少将其用户帐户与其他网站链接，或采用OpenID等单点登录技术，用户可以使用单一用户名登录到不同的网站（例如，用户可以使用其Gmail帐户登录Google+和YouTube）。无论如何，在不同站点之间存在用户名之间的映射，连接其背后的真实身份。我们可以找到这个映射吗？

在本文中，我们引入了一种方法（MOBIUS），用于查找跨社交媒体网站的身份之间的映射。我们的方法是基于用户在社交媒体中展示的行为模式，并且源于社会学和心理学的行为理论。由于环境，人格甚至人为的限制，独特的行为可以在社交媒体网站上创建冗余信息。我们的方法利用这种冗余来识别跨社交媒体网站的用户。我们使用站点之间可用的最少信息量。

第2节在社交媒体网站上正式呈现用户识别问题。第3节描述了用户可以利用社交媒体展示用户识别技术的行为模式。第4节详细说明了识别相应身份的实验，其次是第5节的相关工作。第6节总结了本研究的未来工作方向},
}


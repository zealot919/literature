@Article{2016-ACM-Personalised,
  author   = {Gabriella Kazai/Lumi_News},
  title    = {Personalised News and Blog Recommendations based on User Location, Facebook and Twitter User Profiling 基于用户位置，Facebook和Twitter用户分析的个性化新闻和博客推荐},
  journal  = {SIGIR ’16, July 17-21, 2016, Pisa, Italy},
  year     = {2016},
  abstract = {此演示提供了一个原型移动应用程序通过利用和组合用户的位置，向用户提供开箱即用的个性化内容建议，他们的Facebook和/或Twitter Feed以及他们的应用内动作来自动推断他们的兴趣。
我们为每个用户和每个位置构建单个模型。在检索时，我们通过将不同的基于内容的推荐内容与内容直接从他们的Facebook / Twitter Feed，本地热门文章和通过其应用内社交网络传播的内容混合构建用户的个性化Feed。
用户与其建议的交互的显式和隐含的反馈信号都用于更新他们的兴趣模型，并通过不同的内容来源学习他们的偏好。},
  file     = {2016-ACM-Personalised News and Blog Recommendations based on User Location, Facebook and Twitter User Profiling.pdf:2016-ACM-Personalised News and Blog Recommendations based on User Location, Facebook and Twitter User Profiling.pdf:PDF},
  review   = {只看了实验部分
我们的最终目标是制作一个人们想要每天使用的应用程序：我们希望优化用户保留。
然而，衡量对 留存率 的影响需要很长时间，我们已经找到了一个与之密切相关的指标，
即用户花费超过三十秒阅读的建议数量（从现在开始我们将其称为“读”）。
这是一个用于新闻应用程序的自然指标，它允许我们在尝试不同的推荐算法时迭代更快。

为了说明用户群体的自然变化以及不同用户的阅读倾向，我们对单独的建议使用拆分测试，
以便每个用户从各种来源或算法获取建议。这使我们能够看到有多少用户喜欢将一个源或算法优先于另一个，
提供一种可靠的方式来确定建议是否正常工作。如果我们要在整个用户分割建议，
使一些用户得到了一种类型的建议，而有些用户则有另外一个，这将是很难相信我们的结果，因为有些用户阅读，自然比别人多。

We use a Bayesian formula due to Evan Miller4 to compare two types of recommendations.
数据源：http://www.evanmiller.org/bayesian-abtesting.html},
}

@Article{2016-acm-UserProfilinginthe,
  author   = {Roberto.Gonzalez/NEC.Labs.Europe},
  title    = {User Profiling in the Time of HTTPS},
  journal  = {IMC 2016, November 14-16, 2016, Santa Monica, CA, USA},
  year     = {2016},
  abstract = {跟踪网站内和网站之间的用户是通过有针对性的广告和大数据分析来获取利益，人口统计类型和其他信息的基础。
HTTPS的到来应该使得对于任何超出通信端点的人来说，更难以进行分析。在本文中，我们研究了上述情况在多大程度上是真实的。

我们首先通过了解用户访问的域名，无论是通过TLS协议或通过DNS服务器名称指示，窃听者已经可以得到基本分析的信息，尤其是对于其内容是同质域。
对于依赖于用户访问的特定页面的各种类别的域，例如新闻门户，电子商务站点等，基本分析技术失败。
然而，通过使用的流量指纹识别仍然可以进行准确的分析网络流量签名来推断用户浏览的确切页面，甚至在HTTPS下。
我们证明，传输层指纹识别保持稳健可扩展性，尽管存在诸如缓存，不同设备类型的动态内容等障碍。
总体而言，我们的结果表明，尽管HTTPS使得分析更加困难，但它并没有以任何方式消除它。},
  file     = {2016-acm-UserProfilinginthe Time of HTTPS.pdf:2016-acm-UserProfilinginthe Time of HTTPS.pdf:PDF},
  review   = {仅看摘要，从http协议获取信息},
}

@Article{yin2016,
  author   = {Yifang Yin/National_University_of_Singapore},
  title    = {Encoded Semantic Tree for Automatic UserProfiling Applied to Personalized Video Summarization 用于自动用户分析的 编码语义树 应用于个性化视频摘要},
  journal  = {SUBMISSION TO IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS FOR VIDEO TECHNOLOGY 视频类顶刊},
  year     = {2016},
  abstract = {我们提出一种创新的方法自动视频摘要生成与个人适应。
用户兴趣从他们的个人图像集合中挖掘出来。
为了减少语义差距，我们提出基于新颖的语义树（SeTree）提取视觉表示。SeTree是一个层次结构，可以捕获码本中的视觉场景之间的概念关系。
这个想法建立在以下观点之上：元素之间的这种语义连接在以前的工作中被忽略。
为了构建SeTree，我们采用了一个规范化的图形剪切聚类算法，通过结合使用视觉特征，文本信息和社会用户图像连接。
通过使用这种技术，与现有方法相比，我们在个性化视频段排序中获得了8.1％的归一化折扣累积增益（nDCG）的改善。

此外，为了促进视频的有趣部分，我们提取时空显着图，并通过内核拟合和匹配来估计段的吸引力。
利用线性函数来组合两个因素，基于该因素，视频的重放速率适合于生成摘要。
我们以快速模式播放不太重要的细分，以便用户更新上下文。进行了主观实验，表明我们提出的视频摘要方法胜过最先进的技术6.2％},
  file     = {yin2016.pdf:ieee\\yin2016.pdf:PDF},
  review   = {视频类},
}

@Article{sahil2016,
  author   = {Sahi/Punjab_Institute_of_Technology_Kapurthala},
  title    = {Designing and Analysis of User Profiling System forCloud Computing Security using Fuzzy guided Genetic Algorithm 使用模糊引导遗传算法进行云计算安全用户分析系统的设计与分析},
  journal  = {ICCCA2016},
  year     = {2016},
  abstract = {云计算在一方面以按付费方式提供可扩展的虚拟计算资源，以网络服务的形式; 但另一方面，也引起了对环境外部和内部对云环境的安全威胁的担忧。
现有的安全系统无法承受威胁的动态性质，因此现有的安全系统需要进行某种扩充，这将使安全系统能够以更突出的方式工作。VM之间的攻击在云环境中具有这种威胁。
用户分析在这种情况下可以通过分析用户的使用模式和从安全系统的这些使用模式提供合理的解释来帮助，然后，安全系统基于这些解释，以反应和主动的方式对分析威胁情况。
本文是基于我们最后一篇论文的发现，我们发现基于人工智能技术的用户分析系统的设计：模糊和遗传算法单独不足以提供全面的用户分析系统，但可能是可能的 以混合方式使用这两种方法来设计用于云计算安全性的全面的用户分析系统。因此，我们使用混合方法模糊导向遗传算法设计用户分析系统，发现它涵盖了我们以前的工作分析的所有研究差距，甚至完成了评估框架的所有七个原则},
  file     = {sahil2016.pdf:ieee\\sahil2016.pdf:PDF},
  review   = {无通用数据},
}

@Article{pandey2016,
  author   = {Kriti_Pandey/India},
  title    = {User Profiling on Tumblr through Blog Posts 通过博客文章分析Tumblr用户},
  journal  = {ICCTICT},
  year     = {2016},
  abstract = {Tumblr是一个微博服务，近期获得了26930万个博客和1259亿个帖子，使其成为创意内容的中心。
尽管Tumblr自推出以来急剧增长，但并没有做很多学术工作。这项工作是为了在Tumblr的年龄，性别和关系状态方面全面地了解博客。
本文分析了基于专题分析和写作特征的博客兴趣分类方法。

标准化加权表示最受欢迎的博客类别
被推断基于专题词和定义的类别相关术语之间的相关性。

训练模型预测用户的年龄，性别和关系状况分别为82.37％，79.61％和76.52％。
据我们所知，这是第一次尝试在Tumblr上分析用户},
  file     = {pandey2016.pdf:ieee\\pandey2016.pdf:PDF},
  review   = {III. DATA COLLECTION 此部分主要介绍了如何获取数据
在本节中，对本工作的数据收集中部署的方法进行了简要说明，并对所使用的数据集进行了表征。T
umblr公开提供的API已被用于收集每个数据集，如下文所述的小节所述。
整个数据收集过程跨越了2015年9月5日至2015年10月12日的38天。
数据收集是使用Amazon M4 EC2实例完成的，具有32 GB的RAM和8个vCPU。整个数据收集过程在图1中得到了很好的体现。

在Python中编写了一个广泛的第一搜索（BFS）爬虫，用于启动用5个病毒Tumblr博客[20]作为种子的博客的数据收集。
爬行器首先收集了这些种子的公开暴露的样子反复提取“喜欢喜欢”。在这个过程中，共收集了329,526个博客，我们将其称为“博客”数据集。
除此之外，还包含所有这些博客的标题和描述的元信息也被收集并存储为博客信息数据集},
}

@Article{fan2016,
  author   = {Yao-Chung_Fan/National_Chung_Hsing_University},
  title    = {A Framework for Enabling User Preference Profiling through Wi-Fi Logs},
  journal  = {ICDE 数据库方向顶会},
  year     = {2016},
  abstract = {通过从移动设备收集的Wi-Fi日志来了解用户有两个原始观察。首先，每个Wi-Fi接入点都有一个32位字符串的服务集标识符（SSID）。
Wi-Fi接入点的SSID通常是可读的字符串，因此通常被称为Wi-Fi网络的网络名称。 SSID通常由设置Wi-Fi网络的用户命名。因此，SSID通常具有语义。
第二，当用户靠近WiFi接入点时，产生Wi-Fi SSID。连续观察到的SSID的高频意味着在某个地方长时间停留。
通过这两个观察，我们可以使用带有语义的SSID来推断用户身份和用户偏好等信息。例如，可以从用户每天访问的地方推断用户的占用，例如，研究生可以每个工作日去他/她的实验室。},
  file     = {fan2016.pdf:ieee\\fan2016.pdf:PDF},
}

